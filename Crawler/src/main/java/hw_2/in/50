то.\u003Cbr\u003E\r\n\u003Cbr\u003E\r\n\u003Ca href=\"https:\u002F\u002Fhabr.com\u002Fru\u002Fcompany\u002Fgetmeit\u002Fblog\u002F651579\u002F\"\u003E\u003Cdiv style=\"text-align:center;\"\u003E\u003Cimg src=\"https:\u002F\u002Fhabrastorage.org\u002Fwebt\u002Fws\u002Fqn\u002Frv\u002Fwsqnrvaoiijoil0d7z7nhbrsgto.png\"\u003E\u003C\u002Fdiv\u003E\u003C\u002Fa\u003E\u003Cbr\u003E","imageUrl":null,"buttonTextHtml":"Шах и мат, экстраверты!","image":null},"status":"published","plannedPublishTime":null,"checked":null,"tags":[{"titleHtml":"get me it"},{"titleHtml":"социальное взаимодействие"},{"titleHtml":"интроверты"},{"titleHtml":"общение"},{"titleHtml":"лайфхаки для гиков"}]},"651971":{"id":"651971","timePublished":"2022-02-17T09:14:19+00:00","isCorporative":false,"lang":"ru","titleHtml":"Как укротить Telegram, чтобы он не мешал концентрироваться","editorVersion":"2.0","postType":"article","postLabels":[{"type":"tutorial","data":null}],"author":{"id":"1324372","alias":"m0rtyn","fullname":"Мартын","avatarUrl":"\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Favatars\u002F1b7\u002F3e6\u002F659\u002F1b73e66592a6c05be03316b115c2ffbd.png","speciality":"JS Mentor"},"statistics":{"commentsCount":21,"favoritesCount":32,"readingCount":5281,"score":11,"votesCount":19},"hubs":[{"relatedData":null,"id":"17463","alias":"usability","type":"collective","title":"Usability","titleHtml":"Usability","isProfiled":true},{"relatedData":null,"id":"20686","alias":"community_management","type":"collective","title":"Управление сообществом","titleHtml":"Управление сообществом","isProfiled":true},{"relatedData":null,"id":"21924","alias":"social_networks","type":"collective","title":"Социальные сети и сообщества","titleHtml":"Социальные сети и сообщества","isProfiled":false},{"relatedData":null,"id":"21982","alias":"lifehacks","type":"collective","title":"Лайфхаки для гиков","titleHtml":"Лайфхаки для гиков","isProfiled":false}],"flows":[{"id":"2","alias":"design","title":"Дизайн"},{"id":"3","al