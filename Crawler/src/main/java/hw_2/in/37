у.\u003C\u002Fp\u003E","imageUrl":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002F7dd\u002F1c3\u002F22c\u002F7dd1c322c705f8223263074d72ef2a5e.jpg","buttonTextHtml":"Читать далее","image":{"url":"https:\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Fupload_files\u002F7dd\u002F1c3\u002F22c\u002F7dd1c322c705f8223263074d72ef2a5e.jpg","fit":"cover","positionY":55.454545454545,"positionX":4.2307692307692}},"status":"published","plannedPublishTime":null,"checked":null,"tags":[{"titleHtml":"таиланд"},{"titleHtml":"пхукет"},{"titleHtml":"счёт"},{"titleHtml":"банк"}]}},"articlesIds":{"ARTICLES_LIST_BY_HUB_ALL_FINANCE":["654859","654677","654537","654313","654165","654155","653805","653665","653661","653467","653085","652987","651847","651241","651381","651225","651191","650197","651067","651019"]},"isLoading":false,"pagesCount":{"ARTICLES_LIST_BY_HUB_ALL_FINANCE":50},"route":{"name":"ARTICLES_LIST_BY_HUB_ALL","params":{"hl":"ru","hubName":"finance"},"query":{}},"reasonsList":null,"view":"cards","lastVisitedRoute":{},"ssrCommentsArticleIds":[],"karma":{}},"authorContribution":{"authors":{"hub_finance":[{"scoreStats":{"score":300,"votesCount":1864},"rating":10,"invest":5086.8,"relatedData":null,"id":"43347","alias":"marks","fullname":"Максим Агаджанов","avatarUrl":"\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Favatars\u002F877\u002F493\u002Fd58\u002F877493d580304894f45ddf1a4844de04.jpg","speciality":"Редактор","lastPost":null},{"scoreStats":{"score":923,"votesCount":4059},"rating":17.2,"invest":3815.2,"relatedData":null,"id":"5","alias":"alizar","fullname":"Анатолий Ализар","avatarUrl":"\u002F\u002Fhabrastorage.org\u002Fgetpro\u002Fhabr\u002Favatars\u002Fcc3\u002F97d\u002F54c\u002Fcc397d54c2161005908a3034c5fabbea.png","speciality":"автор, фрилансер","lastPost":null},{"scoreStats":{"score":-27,"votesCount":141},"rating":0,"invest":2817.4,"relatedData":null,"id":"471760","alias":"ITI_Capital","fullname":null,"avatarUrl":"\u002F\u